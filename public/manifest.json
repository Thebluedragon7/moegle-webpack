{
  "manifest_version": 3,
  "name": "Moegle",
  "short_name": "Moegle",
  "version": "1.0",
  "version_name": "1.0a",
  "description": "",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "./icons/ico16.png",
    "32": "./icons/ico32.png",
    "48": "./icons/ico48.png",
    "64": "./icons/ico64.png",
    "128": "./icons/ico128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.omegle.com/"],
      "js": ["JS/content.js"]
    }
  ],
  "offline_enabled": false,
  "action": {
    "default_title": "Moegle: Omegle Utility",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icons/ico16.png",
      "32": "./icons/ico32.png",
      "48": "./icons/ico48.png",
      "64": "./icons/ico64.png",
      "128": "./icons/ico128.png"
    }
  }
}
